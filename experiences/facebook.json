{
	"cards": {
		"facebook": {
			"content": {
				"type": "AMPDocument",
				"url": "${AMP_DOCUMENT_URL}"
			},
			"snapPoints": {
				"active": 0.52,
				"initial": 0.52,
				"minimised": 1
			},
			"features": {
				"mode": "contextual",
				"isDraggable": true
			},
			"triggers": {
				"myScrollTrigger": {
					"on": "scroll",
					"spec": {
						"type": "absolute",
						"pixel": 300,
						"percentage": 0.1
					}
				}
			}
		}
	}
}
